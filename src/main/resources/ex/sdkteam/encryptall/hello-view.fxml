<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="520.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ex.sdkteam.encryptall.MainController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <MenuBar prefHeight="0.0" prefWidth="476.0">
     <menus>
       <Menu mnemonicParsing="false" text="File">
         <items>
           <MenuItem mnemonicParsing="false" text="Close" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Edit">
         <items>
           <MenuItem mnemonicParsing="false" text="Delete" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Help">
         <items>
           <MenuItem mnemonicParsing="false" text="About" />
         </items>
       </Menu>
     </menus>
   </MenuBar>
   <Label prefHeight="16.0" prefWidth="268.0" text="Enter the text you would like to encrypt:">
      <VBox.margin>
         <Insets />
      </VBox.margin></Label>
   <TextArea prefHeight="136.0" prefWidth="481.0" wrapText="true" />
   <HBox prefHeight="85.0" prefWidth="480.0" spacing="100.0">
      <Button fx:id="Submit_Button" mnemonicParsing="false" prefHeight="36.0" prefWidth="78.0" text="Submit" />
      <Button fx:id="Refresh_Button" mnemonicParsing="false" prefHeight="36.0" prefWidth="72.0" text="Refresh" />
      <MenuButton mnemonicParsing="false" prefHeight="36.0" prefWidth="128.0" text="MenuButton">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </MenuButton>
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets top="15.0" />
      </padding>
   </HBox>
   <TextArea disable="true" editable="false" prefHeight="200.0" prefWidth="480.0" />
</VBox>
